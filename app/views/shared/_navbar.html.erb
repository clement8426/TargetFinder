<dh-component data-controller="side-bar" class="fixed top-0 left-0 z-10" style="height: 100dvh; >
    <div class="flex flex-no-wrap" style="height: 100dvh;">
      <div style="min-height: 716px" class="fixed w-64 absolute sm:relative bg-gray-800 shadow md:h-full flex-col justify-between hidden sm:flex">
          <div class="px-8 h-full w-full">
            <div class="h-16 w-full flex items-center">
              <%= image_tag 'logo.png' %>

            </div>
            <input class="mt-6 bg-gray-700 focus:outline-none focus:ring-1 focus:ring-gray-100 rounded w-full text-sm text-gray-300 placeholder-gray-400 bg-gray-100 pl-10 py-2" type="text" placeholder="Search" />
            <div class="h-4/5 flex flex-col justify-between">
              <div id="1">
                <ul class="mt-12">
                    <li class="flex w-full justify-between text-gray-300 cursor-pointer items-center mb-6">
                      <%= link_to root_path, class: "flex items-center focus:outline-none focus:ring-2 focus:ring-white" do %>
                            <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-puzzle" width="18" height="18" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                <path stroke="none" d="M0 0h24v24H0z"></path>
                                <path d="M11 18l-2 -1l-6 3v-13l6 -3l6 3l6 -3v7.5" /><path d="M9 4v13" /><path d="M15 7v5" /><path d="M18 18m-3 0a3 3 0 1 0 6 0a3 3 0 1 0 -6 0" /><path d="M20.2 20.2l1.8 1.8" />
                            </svg>
                            <span class="text-sm ml-2">Carte</span>
                      <% end %>
                    </li>
                    <li class="flex w-full justify-between text-gray-400 hover:text-gray-300 cursor-pointer items-center mb-6">
                      <%= link_to clubs_path, class: "flex items-center focus:outline-none focus:ring-2 focus:ring-white" do %>
